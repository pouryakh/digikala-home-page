{
  "name": "digikala",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "compile:sass-w": "sass sass/main.scss public/css/style.css -w",
    "compile:sass": "sass sass/main.scss public/css/style.compile.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 version' public/css/style.compile.css -o public/css/style.prefix.css",
    "comp:css": "sass public/css/style.prefix.css public/css/style.css --style=compressed",
    "build:css": "npm-run-all compile:sass prefix:css comp:css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "sass": "^1.93.2",
    "swiper": "^12.0.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "bootstrap": "^5.3.8",
    "jquery": "^3.7.1"
  }
}
